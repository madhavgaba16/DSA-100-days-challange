#include <iostream>
#include <limits.h>

int main() {
    int arr[] = {1, 2, 3, 4, 5};
    int size = sizeof(arr) / sizeof(int);
    int min = INT_MAX;

    for (int i = 0; i < size; i++) {
        if (min > arr[i]) {
            min = arr[i];                              // finding miminum element in the array 
            
        }
    }    

    std::cout << min << std::endl;
    return 0;
}